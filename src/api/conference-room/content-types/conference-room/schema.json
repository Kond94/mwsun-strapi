{
  "kind": "collectionType",
  "collectionName": "conference_rooms",
  "info": {
    "singularName": "conference-room",
    "pluralName": "conference-rooms",
    "displayName": "Conference-Room"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "capacity": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "displayPhoto": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "photos": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "amenities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::amenity.amenity"
    }
  }
}
